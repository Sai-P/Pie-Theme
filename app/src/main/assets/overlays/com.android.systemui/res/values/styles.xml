<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="qs_theme" parent="@*com.android.systemui:style/qs_base">
        <item name="*com.android.systemui:darkIconTheme">@*com.android.systemui:style/QSIconTheme</item>
        <item name="*com.android.systemui:lightIconTheme">@*com.android.systemui:style/QSIconTheme</item>
        <item name="android:colorBackgroundFloating">@*android:color/primary_material_dark</item>   <!--OG:@color/background_floating_material_dark-->
        <item name="android:colorControlNormal">#ffffffff</item>    <!--OG:?textColorPrimary-->
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>    <!--OG:@color/primary_device_default_settings_light-->
        <item name="android:colorPrimaryDark">@*android:color/background_material_dark</item>    <!--OG:@color/primary_dark_device_default_settings_light-->
        <item name="android:colorAccent">@*android:color/material_deep_teal_500</item>
        <item name="android:colorSecondary">@*android:color/background_material_dark</item>     <!--OG:@color/secondary_device_default_settings_light-->
    </style>
	
    <style name="TextAppearance.NotificationInfo.Secondary.Link" parent="@*com.android.systemui:style/TextAppearance.NotificationInfo.Secondary">
        <item name="android:textColor">@*android:color/material_deep_teal_500</item>   <!--OG:?android:colorAccent-->
    </style>
    <style name="TextAppearance.NotificationInfo.Button" parent="@*com.android.systemui:style/TextAppearance.NotificationInfo">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@*android:color/material_deep_teal_500</item>   <!--OG:?android:colorAccent-->
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
</resources>
