<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetLeft="@*com.google.android.calculator:dimen/abc_button_inset_horizontal_material"
    android:insetTop="@*com.google.android.calculator:dimen/abc_button_inset_vertical_material"
    android:insetRight="@*com.google.android.calculator:dimen/abc_button_inset_horizontal_material"
    android:insetBottom="@*com.google.android.calculator:dimen/abc_button_inset_vertical_material">
    <shape android:shape="rectangle">
        <corners android:radius="@*com.google.android.calculator:dimen/abc_control_corner_material" />
        <solid android:color="@*android:color/background_floating_material_dark" />
        <padding
            android:bottom="@*com.google.android.calculator:dimen/abc_button_padding_vertical_material"
            android:left="@*com.google.android.calculator:dimen/abc_button_padding_horizontal_material"
            android:right="@*com.google.android.calculator:dimen/abc_button_padding_horizontal_material"
            android:top="@*com.google.android.calculator:dimen/abc_button_padding_vertical_material" />
    </shape>
</inset>
